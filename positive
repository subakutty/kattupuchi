# kattupuchi
import java.util.*;
class PosNegZero
{
  public Static void main(String args[])
  {
    int num;
    Scanner sc=new Scanner(System.in);
    System.out.println("enter the number:");
    num=sc.nextInt();
    if(num>0)
        System.out.println("positive");
    else if(num<0)
        System.out.println("negative");
    else
     System.out.println("zero");
    }
   }
